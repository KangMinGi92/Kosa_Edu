package com.edu.child;

import com.edu.util.MyDate;

public class Secretary {
	private String nameOfBoss;
	public Engineer(int empno, String name, double salary, MyDate birthDate, String tech, int bonus) {
		super(empno, name, salary, birthDate);
		this.tech = tech;
		this.bonus = bonus;
	}
	public String getNameOfBoss() {
		return nameOfBoss;
	}

	public void setNameOfBoss(String nameOfBoss) {
		this.nameOfBoss = nameOfBoss;
	}
	
	
	
}
