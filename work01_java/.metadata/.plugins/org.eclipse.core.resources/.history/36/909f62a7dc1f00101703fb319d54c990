package com.edu.child;

public class Engineer {
	private String tech;
	private int bonus;
	
	public Engineer() {
		// TODO Auto-generated constructor stub
	}

	public Engineer(String tech, int bonus) {
		super();
		this.tech = tech;
		this.bonus = bonus;
	}

	public String getTech() {
		return tech;
	}

	public void setTech(String tech) {
		this.tech = tech;
	}

	public int getBonus() {
		return bonus;
	}

	public void setBonus(int bonus) {
		this.bonus = bonus;
	}

	@Override
	public String toString() {
		return "Engineer [tech=" + tech + ", bonus=" + bonus + "]";
	}
	
}
